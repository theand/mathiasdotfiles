export MANPATH="$(brew --prefix)/opt/findutils/libexec/gnuman:$(brew --prefix)/opt/coreutils/libexec/gnuman:$(brew --prefix)/opt/gnu-sed/libexec/gnuman:$(brew --prefix)/opt/gnu-tar/libexec/gnuman:$MANPATH";


